\n    resetTaskAction({commit}){\n        console.log(\"asd\")\n        commit('resetTask')\n    }\n};\n\nconst mutations = {\n    setTaskLikes:(state,task)=>{    \n        let editTask;\n        for(let t of state.tasks){\n            if(t._id == task._id){\n                t.likes = task.likes\n                console.log(editTask = t)\n            }\n        }\n        console.log(editTask)\n    },\n    setTasks: (state, tasks) => {\n        return state.tasks = tasks.data\n    },\n    updateTask: (state, task) => {\n        return state.tasks.push(task.data)\n    },\n    setTask: (state, task) => {\n        return state.task = task.data\n    },\n    deleteTask:(state,task)=>{\n        let newTask=[]\n        state.tasks.map(t=>{\n            if(task._id != t._id){\n                newTask.push(t)\n            }\n        })\n        state.tasks = newTask\n        return state.tasks\n    },\n    resetTask:(state)=>{\n        state.task = {}\n    }\n         \n    \n};\nconst getters = {\n    getTasks: (state) => {\n        return state.tasks\n    },\n    getTask: (state) => {\n        return state.task\n    }\n\n};\n\nexport default {\n    state,\n    actions,\n    mutations,\n    getters\n}"]}]}