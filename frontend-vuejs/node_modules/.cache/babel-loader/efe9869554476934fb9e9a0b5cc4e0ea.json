s=\"mt--8 pb-5\">\n      <b-row class=\"justify-content-center\">\n        <b-col lg=\"5\" md=\"7\">\n          <b-card no-body class=\"bg-light border-0 mb-0\">\n            <b-card-header class=\"bg-transparent pb-5\"> </b-card-header>\n            <b-card-body class=\"px-lg-5 py-lg-5\">\n              \n              <div class=\"row\">\n                <div class=\"col-md-12  col-xs-12\">\n                  <h1 class=\"text-xs-center\">Create Account</h1>\n                  <p class=\"text-xs-center\">\n                  </p>\n                  <div v-if=\"errors\">\n                  <b-alert variant=\"danger\"  v-for=\"(v, k) in errors\" :key=\"k\" show> {{v}} </b-alert>\n              </div>\n                  <form >\n                      <fieldset class=\"form-group\">\n                      <input\n                        class=\"form-control form-control-lg\"\n                        type=\"text\"\n                        v-model=\"fullName\"\n                        placeholder=\"Full Name\"\n                      />\n                    </fieldset>\n                    <fieldset class=\"form-group\">\n                      <input\n                        class=\"form-control form-control-lg\"\n                        type=\"text\"\n                        v-model=\"email\"\n                        placeholder=\"Email\"\n                      />\n                    </fieldset>\n                    <fieldset class=\"form-group\">\n                      <input\n                        class=\"form-control form-control-lg\"\n                        type=\"password\"\n                        v-model=\"password\"\n                        placeholder=\"Password\"\n                      />\n                    </fieldset>\n                    \n                    <button @click=\"onSubmit(password,email,fullName)\" class=\"btn btn-lg btn-primary pull-xs-right\">\n                      Create Account\n                    </button>\n                  </form>\n                </div>\n              </div>\n            </b-card-body>\n          </b-card>\n          <b-row class=\"mt-3\">\n            <b-col cols=\"6\" class=\"text-right\">\n              <router-link to=\"/register\" class=\"text-light\"\n                ><small>Create new account</small></router-link\n              >\n            </b-col>\n          </b-row>\n        </b-col>\n      </b-row>\n    </b-container>\n  </div>\n</template>\n\n<script>\nimport { mapState } from 'vuex';\nexport default {\n    name:\"Register\",\n    data(){\n        return {\n            password:'',\n            email:'',\n            fullName:'',\n        }\n    },\n    methods:{\n        onSubmit(pasword,email,fullName){\n            const user = {\n                password:pasword,\n                email:email,\n                name:fullName\n            }\n            this.$store.dispatch('register',user)\n        .then(() => this.$router.push({ name: \"home\" }));\n\n        }\n    },\n    computed:{\n        ...mapState({\n            errors:state=>state.auth.errors\n        })\n    }\n}\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/components"}]}