n      </b-row>\n      <b-row\n        class=\"table-row\"\n        v-bind:key=\"index\"\n        v-for=\"(alert, index) in task.alerts\"\n      >\n        <b-col>\n          <div class=\"row\">\n            <div class=\"col-md-12 col-style\">\n              <div class=\"col-md-3\">\n                <div class=\"col-md-12 col-style\">\n                  <div class=\"col-md-4\">\n                    <img style=\"width:100%;heigth:100%\" />\n                  </div>\n                  <div class=\"col-md-8\">\n                    <label for=\"\">\n                      {{ alert.user.name }}\n                    </label>\n                    <label class=\"date-style\" >\n                      {{\n                        alert.createdAt | moment(\"dd, MMMM Do YY hh:mm:ss\")\n                      }}\n                    </label>\n                  </div>\n                </div>\n              </div>\n              <div class=\"col-md-9\" style=\"display:flex\">\n                {{ alert.content }}\n              </div>\n            </div>\n          </div>\n        </b-col>\n      </b-row>\n    </b-container>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from \"vuex\";\nexport default {\n  name: \"Task-Detail\",\n  props: {\n    taskId: String,\n    task: Object,\n    vm: Object,\n  },\n  computed: {\n    ...mapGetters({\n      task: \"getTask\",\n    }),\n  },\n  created() {\n    this.$store.dispatch(\"fetchTask\", this.taskId);\n  },\n    methods:{\n        logout(){\n            this.$store.dispatch('logout')\n            .then(()=> this.$router.push({ name: \"login\" }))\n        }\n    }\n};\n</script>\n\n<style scoped>\n.col-style {\n  display: flex;\n}\n.table-row:nth-of-type(odd) {\n  background: #fff;\n}\n.table-row:nth-of-type(even) {\n  background: #edf1f7;\n}\n.date-style {\n  font-size: 0.74rem;\n}\n</style>\n"]}]}